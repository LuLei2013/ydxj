<!--pages/main/main.wxml-->
<view>
   <swiper indicator-dots="{{banner.indicatorDots}}"
    autoplay="{{banner.autoplay}}" 
    interval="{{banner.interval}}" 
    duration="{{banner.duration}}">
    <block wx:for="{{banner.bannerImgUrls}}" 
      wx:for-item="item">
      <swiper-item>
        <image src="{{item}}" 
          class="swiper-img"/>
      </swiper-item>
    </block>
  </swiper>  
</view>